      <memberdef kind="enum" id="image_8h_1a8cb59f5925c96a706710106f679d171a" prot="public" static="no">
        <name>wxImageResizeQuality</name>
        <enumvalue id="image_8h_1a8cb59f5925c96a706710106f679d171aac8930a29f369ca64902465915359f7b9" prot="public">
          <name>wxIMAGE_QUALITY_NEAREST</name>
          <briefdescription>
<para>Simplest and fastest algorithm. </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="image_8h_1a8cb59f5925c96a706710106f679d171aa4ae1f01f78c26d0de7704b1e7c45b509" prot="public">
          <name>wxIMAGE_QUALITY_BILINEAR</name>
          <briefdescription>
<para>Compromise between wxIMAGE_QUALITY_NEAREST and wxIMAGE_QUALITY_BICUBIC. </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="image_8h_1a8cb59f5925c96a706710106f679d171aa4fd9eb406fcb636da675fd96447a7540" prot="public">
          <name>wxIMAGE_QUALITY_BICUBIC</name>
          <briefdescription>
<para>Highest quality but slowest execution time. </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="image_8h_1a8cb59f5925c96a706710106f679d171aaca0af9f66d038da9d6ab7e2838d705e4" prot="public">
          <name>wxIMAGE_QUALITY_BOX_AVERAGE</name>
          <briefdescription>
<para>Use surrounding pixels to calculate an average that will be used for new pixels. </para>          </briefdescription>
          <detaileddescription>
<para>This method is typically used when reducing the size of an image. </para>          </detaileddescription>
        </enumvalue>
        <enumvalue id="image_8h_1a8cb59f5925c96a706710106f679d171aacfa5fa2a8feb97568b19f5784d6ac5e3" prot="public">
          <name>wxIMAGE_QUALITY_NORMAL</name>
          <briefdescription>
