          <briefdescription>
<para><emphasis>R</emphasis> = 0 </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="graphics_8h_1af26ae47387aa983f2a975b3f85cb9668a19a55339fe50cf87cc40e6f675bc8150" prot="public">
          <name>wxCOMPOSITION_SOURCE</name>
          <briefdescription>
<para><emphasis>R</emphasis> = S </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="graphics_8h_1af26ae47387aa983f2a975b3f85cb9668a46fea70da2a1f8cb1151951639c8d07f" prot="public">
          <name>wxCOMPOSITION_OVER</name>
          <briefdescription>
<para><emphasis>R</emphasis> = <emphasis>S</emphasis> + <emphasis>D*</emphasis>(1 - <emphasis>Sa</emphasis>) </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="graphics_8h_1af26ae47387aa983f2a975b3f85cb9668ad30d4e9dbf0d0367a749a952ec76be6b" prot="public">
          <name>wxCOMPOSITION_IN</name>
          <briefdescription>
<para><emphasis>R</emphasis> = <emphasis>S*<emphasis>Da</emphasis> </emphasis> </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="graphics_8h_1af26ae47387aa983f2a975b3f85cb9668a24a1b11099bbce5e64206d8de2a4014e" prot="public">
          <name>wxCOMPOSITION_OUT</name>
          <briefdescription>
<para><emphasis>R</emphasis> = <emphasis>S*</emphasis>(1 - <emphasis>Da</emphasis>) </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="graphics_8h_1af26ae47387aa983f2a975b3f85cb9668ae96c0aab326b220e4d5961dd43b10446" prot="public">
          <name>wxCOMPOSITION_ATOP</name>
          <briefdescription>
<para><emphasis>R</emphasis> = <emphasis>S*<emphasis>Da</emphasis> +</emphasis> <emphasis>D*</emphasis>(1 - <emphasis>Sa</emphasis>) </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="graphics_8h_1af26ae47387aa983f2a975b3f85cb9668a504c9465521237897f92d78fc3e13433" prot="public">
          <name>wxCOMPOSITION_DEST</name>
          <briefdescription>
<para><emphasis>R</emphasis> = <emphasis>D</emphasis>, essentially a noop </para>          </briefdescription>
          <detaileddescription>
          </detaileddescription>
        </enumvalue>
        <enumvalue id="graphics_8h_1af26ae47387aa983f2a975b3f85cb9668ab2bb23f40fbb928f23ae876ad51341b5" prot="public">
          <name>wxCOMPOSITION_DEST_OVER</name>
          <briefdescription>
