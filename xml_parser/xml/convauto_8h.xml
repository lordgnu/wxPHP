        <name>wxBOM</name>
        <enumvalue id="convauto_8h_1af393832da543c9238919f38be4536f60afc832b9aa62fb80a324a55c50d7abf10" prot="public">
          <name>wxBOM_Unknown</name>
          <initializer> -1</initializer>
          <briefdescription>
<para>Unknown BOM. </para>          </briefdescription>
          <detaileddescription>
<para>This is returned if BOM presence couldn&apos;t be determined and normally happens because not enough bytes of input have been analysed. </para>          </detaileddescription>
        </enumvalue>
        <enumvalue id="convauto_8h_1af393832da543c9238919f38be4536f60a549e0aa1904ac2be88f375315665e0ac" prot="public">
          <name>wxBOM_None</name>
          <briefdescription>
<para>No BOM. </para>          </briefdescription>
          <detaileddescription>
<para>The stream doesn&apos;t contain BOM character at all. </para>          </detaileddescription>
        </enumvalue>
        <enumvalue id="convauto_8h_1af393832da543c9238919f38be4536f60a1373305c38d5fa750d0b1255dcdf5ea4" prot="public">
          <name>wxBOM_UTF32BE</name>
          <briefdescription>
<para>UTF-32 big endian BOM. </para>          </briefdescription>
          <detaileddescription>
<para>The stream is encoded in big endian variant of UTF-32. </para>          </detaileddescription>
        </enumvalue>
        <enumvalue id="convauto_8h_1af393832da543c9238919f38be4536f60a13cc70351990fb0b9c622afcded2d1c2" prot="public">
