<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "../pear/package.dtd">
<package>
 <name>wxWidgets</name>
 <summary>PHP bindings to the GUI toolkit library wxWidgets</summary>
 <maintainers>
  <maintainer>
   <user>seariver</user>
   <name>Mário Soares</name>
   <email>seariver@gmail.com</email>
   <role>lead</role>
  </maintainer>
  <maintainer>
   <user>jgmylm</user>
   <name>Jefferson González</name>
   <email>jgmdev@gmail.com</email>
   <role>developer</role>
  </maintainer>
